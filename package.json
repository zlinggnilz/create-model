{
  "name": "create-model",
  "author": "Ling",
  "version": "0.1.1",
  "scripts": {
    "dev": "father-build --w",
    "build": "father-build",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "dva": "^2.X",
    "qs": "^6.X"
  },
  "devDependencies": {
    "@types/qs": "^6.9.7",
    "@umijs/test": "^3.0.5",
    "father-build": "^1.19.1",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  },
  "description": "> based on dva",
  "keywords": [
    "dva",
    "model"
  ],
  "license": "MIT"
}
